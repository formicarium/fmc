{
  "name": "fmc",
  "description": "A CLI to operate on Formicarium",
  "version": "0.0.1",
  "author": "Rafael Ribeiro @rafaelcorreiapoli",
  "bin": {
    "fmc": "./bin/run"
  },
  "bugs": "https://github.com/formicarium/formicarium-cli/issues",
  "dependencies": {
    "@blueprintjs/core": "^3.2.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.18.0",
    "child-process-promise": "^2.2.1",
    "cli-ux": "^4.7.3",
    "fs-extra": "^6.0.1",
    "gitignore-parser": "^0.0.2",
    "graphql": "^0.13.2",
    "graphql-request": "^1.6.0",
    "graphql-tools": "^3.1.1",
    "inquirer": "^6.1.0",
    "lowdb": "^1.0.0",
    "moment": "^2.22.2",
    "node-watch": "^0.5.8",
    "ora": "^3.0.0",
    "ps-node": "^0.1.6",
    "ramda": "^0.25.0",
    "signale": "^1.2.1",
    "simple-git": "^1.96.0",
    "tslib": "^1",
    "tty-table": "^2.6.8",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4",
    "@types/fs-extra": "^5.0.3",
    "@types/get-port": "^3.2.0",
    "@types/graphql": "^0.13.4",
    "@types/inquirer": "^0.0.42",
    "@types/lowdb": "^1.0.5",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ora": "^1.3.4",
    "@types/ramda": "^0.25.34",
    "@types/signale": "^1.2.0",
    "@types/xml2js": "^0.4.3",
    "globby": "^8"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/formicarium/formicarium-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fmc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "formicarium/formicarium-cli",
  "scripts": {
    "readme": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "check-typescript": "tsc --pretty",
    "watch": "tsc -w"
  },
  "types": "lib/index.d.ts"
}
